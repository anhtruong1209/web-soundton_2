    $(document).ready(function() {
        $(".p-tabLink").each(function(){
            $(this).click(function(){
                tabeId = $(this).attr('id');
                $(".p-tabLink").removeClass("p-activeLink");
                $(this).addClass("p-activeLink");
                $(".p-tabcontent").addClass("p-hide");
                $("#"+tabeId+"-form").removeClass("p-hide");
                return false;
            });
        });
        
        $('.gallery-show').fancybox({
